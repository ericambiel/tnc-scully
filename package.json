{
  "name": "tnc-scully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration=production && scully --prod --scanRoutes && npm run pwa:regen",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "pwa:regen": "rm dist/static/ngsw.json;node_modules/.bin/ngsw-config dist/static ./ngsw-config.json"
  },
  "private": true,
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "htmlWhitespaceSensitivity": "ignore"
  },
  "dependencies": {
    "@angular/animations": "12.2.4",
    "@angular/common": "12.2.4",
    "@angular/compiler": "12.2.4",
    "@angular/core": "12.2.4",
    "@angular/forms": "12.2.4",
    "@angular/platform-browser": "12.2.4",
    "@angular/platform-browser-dynamic": "12.2.4",
    "@angular/router": "12.2.4",
    "@angular/service-worker": "12.2.4",
    "@ng-web-apis/common": "1.12.0",
    "@ng-web-apis/intersection-observer": "2.1.0",
    "@notionhq/client": "^0.3.1",
    "@scullyio/init": "1.1.4",
    "@scullyio/ng-lib": "1.1.1",
    "@scullyio/scully": "1.1.1",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.4",
    "@angular/cli": "12.2.4",
    "@angular/compiler-cli": "12.2.4",
    "@gammastream/scully-plugin-http404": "1.0.4",
    "@notiz/scully-plugin-lazy-images": "0.5.0",
    "@notiz/scully-plugin-rss": "1.2.0",
    "@types/gtag.js": "0.0.7",
    "@types/node": "14.14.37",
    "autoprefixer": "10.3.4",
    "cross-env": "7.0.3",
    "html-minifier-terser": "6.0.2",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "scully-plugin-time-to-read": "0.0.5",
    "tailwindcss": "2.2.9",
    "ts-node": "10.2.1",
    "tslint": "~6.1.3",
    "typescript": "4.3.5"
  }
}
